set<int> factors;
while(n%2==0){
	factors.insert(2);	
	n/=2;
}
for(int i=3;i*i<=n;i++){
	while(n%i==0){
		factors.insert(i);
		n/=i;
	}
}
if(n>1)
	factors.insert(n);
vector<int>factors1(all(factors));
]]></content>
